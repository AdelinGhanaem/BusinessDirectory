        <b:customTab>
          <!--<g:Image addStyleNames="{style.icon}" resource="{res.logo}"/>-->
        </b:customTab>
        <b:Heading size="3">Typically Tab</b:Heading>
        <b:Paragraph>
          huhuhu hahha
        </b:Paragraph>
      </b:Tab>
